{
  "name": "3d-model-viewer",
  "version": "0.0.1",
  "description": "360 3d model viewer in Three js loading glTF files",
  "main": "index.js",
  "scripts": {
    "build:production": "(rm -rf ./build/* && env NODE_ENV=production node_modules/.bin/webpack --config=config/webpack.config.js --mode=production)",
    "dev": "(rm -rf ./dist/build/* && env NODE_ENV=development node_modules/.bin/webpack --config=config/webpack.config.js --mode=development --watch)",
    "start": "live-server --open=dist/build/index.html --watch=dist/build"
  },
  "author": "Artur Jakiel",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.1.0",
    "html-webpack-plugin": "^4.3.0",
    "live-server": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "three": "^0.116.1"
  }
}
